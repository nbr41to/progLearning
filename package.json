{
  "name": "proglearning",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fmt": "npx sort-package-json package.json && next lint --fix && prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "prisma-merge": "npx prisma-merge --baseFile prisma/base.prisma --schemaFilePatterns 'prisma/*/*.prisma' --outputFile prisma/schema.prisma",
    "seed": "ts-node --compilerOptions '{\"isolatedModules\":false}' prisma/seed.ts",
    "server": "go run ./go/main.go",
    "start": "next start",
    "studio": "prisma studio"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn fmt"
    ],
    "pages/**/*.{js,jsx,ts,tsx}": [
      "yarn fmt"
    ]
  },
  "dependencies": {
    "@magic-ext/oauth": "^2.1.1",
    "@magic-sdk/admin": "^1.4.1",
    "@notionhq/client": "^1.0.4",
    "@prisma/client": "^4.0.0",
    "axios": "^0.27.2",
    "clsx": "^1.2.0",
    "magic-sdk": "^8.1.1",
    "next": "12.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-syntax-highlighter": "^15.5.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "18.0.14",
    "@types/react-syntax-highlighter": "^15.5.2",
    "autoprefixer": "^10.4.7",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "prisma": "^4.0.0",
    "tailwindcss": "^3.1.4",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "volta": {
    "node": "16.13.0"
  },
  "prisma": {
    "seed": "ts-node --compilerOptions '{\"isolatedModules\":false}' prisma/seed.ts",
    "studio": "prisma studio",
    "merge": "npx prisma-merge --baseFile prisma/base.prisma --schemaFilePatterns 'prisma/*/*.prisma' --outputFile prisma/schema.prisma"
  }
}
